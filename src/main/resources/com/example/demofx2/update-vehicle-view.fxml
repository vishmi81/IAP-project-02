<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demofx2.UpdateController">
   <children>
      <Label layoutX="178.0" layoutY="14.0" text="Update Vehicle" textFill="#ac1919">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtid" layoutX="74.0" layoutY="73.0" prefHeight="35.0" prefWidth="302.0" promptText="Enter id to search" />
      <TextField fx:id="txtmodel" layoutX="74.0" layoutY="182.0" prefHeight="35.0" prefWidth="302.0" promptText="Enter model" />
      <TextField fx:id="txtbrand" layoutX="74.0" layoutY="132.0" prefHeight="35.0" prefWidth="302.0" promptText="Enter brand" />
      <TextField fx:id="txtprice" layoutX="74.0" layoutY="282.0" prefHeight="35.0" prefWidth="302.0" promptText="Enter price" />
      <TextField fx:id="txtgear" layoutX="74.0" layoutY="233.0" prefHeight="35.0" prefWidth="302.0" promptText="Enter no of gear " />
      <Button layoutX="397.0" layoutY="73.0" mnemonicParsing="false" onAction="#searchbtn" prefHeight="35.0" prefWidth="99.0" text="Search" />
      <Button layoutX="74.0" layoutY="334.0" mnemonicParsing="false" onAction="#updatebtn" prefHeight="35.0" prefWidth="99.0" text="Update" />
      <Button layoutX="326.0" layoutY="334.0" mnemonicParsing="false" onAction="#cancelbtn" prefHeight="35.0" prefWidth="99.0" text="cancel" />
   </children>
</AnchorPane>
